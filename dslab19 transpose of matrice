#include <stdio.h>

int main() {
    int a[3][3],m,n,i,j,b[3][3];

    printf("Enter the number of rows and columns of the first array\n");
    scanf("%d%d", &m, &n);
    

  

    printf("Enter the elements of array 1\n");
    for (i = 0; i < m; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    

   
        for (i = 0; i < m; i++) {
            for (j= 0; j < n; j++) {
             b[j][i]=a[i][j];   
            }
        }
        
        printf("Resultant matrix:\n");
        for (i = 0; i<n; i++) {
            for (j = 0; j< m; j++) {
                printf("%d ", b[i][j]);
            }
            printf("\n");
        }
    } 
